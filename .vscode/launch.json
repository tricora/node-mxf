{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Launch Program",
                "program": "${workspaceRoot}/dist/index.js",
                "sourceMaps": true,
                "outFiles": [
                    "${workspaceRoot}/dist/**/*.js"
                ],
                "runtimeExecutable": "npm",
                "runtimeArgs": [
                    "run-script", "debug"
                ],
                "port": 5858,
                "protocol": "inspector",
                "smartStep": true
            }
        ]
    }